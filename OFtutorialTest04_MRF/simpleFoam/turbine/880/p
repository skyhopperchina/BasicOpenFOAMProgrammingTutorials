FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "880";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    HUB
    {
        type            zeroGradient;
    }
    SHROUD
    {
        type            zeroGradient;
    }
    INFLOW
    {
        type            zeroGradient;
    }
    OUTFLOW
    {
        type            fixedValue;
        value           uniform 0;
    }
    BLADE
    {
        type            zeroGradient;
    }
    PER1
    {
        type            cyclicAMI;
        value           uniform 0;
    }
    PER2
    {
        type            cyclicAMI;
        value           uniform 0;
    }
}


